<VirtualHost *:80>
  ServerAdmin tim@linfiniti.com
  ServerName inasafe-nightly.linfiniti.com

  DocumentRoot /home/web/inasafe-nightly/
  DirectoryIndex plugins.xml

  <Directory /home/web/inasafe-nightly/>
    Order deny,allow
    Allow from all
  </Directory>

  ErrorLog /var/log/apache2/inasafe-nightly.linfiniti.error.log
  CustomLog /var/log/apache2/inasafe-nightly.linfiniti.access.log combined
  ServerSignature Off

</VirtualHost>
